<settings xmlns="http://maven.apache.org/SETTINGS/1.0.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.0.0 http://maven.apache.org/xsd/settings-1.0.0.xsd">

	<!-- Id: com.sap.ldi:nexus-settings:1.0.9:LeanDI -->
	
	<proxies>
   <proxy>
      <active>true</active>
      <protocol>http</protocol>
      <host>proxy</host>
      <port>8080</port>
      <nonProxyHosts>nexus</nonProxyHosts>
    </proxy>
       <proxy>
      <active>true</active>
      <protocol>https</protocol>
      <host>proxy</host>
      <port>8080</port>
      <nonProxyHosts>nexus</nonProxyHosts>
    </proxy>
    </proxies>

	
	<mirrors>
		<mirror>
			<id>mirror1</id>
			<url>http://nexus:8081/nexus/content/groups/build.milestones/</url>
			<mirrorOf>*,!build.snapshots.repo,!build.milestones.repo,!build.releases.repo</mirrorOf>

		</mirror>
	</mirrors>
	<profiles>
		<profile>
			<id>release.build</id>
			<pluginRepositories>
				<pluginRepository>
					<id>build.releases.repo</id>

					<url>http://nexus:8081/nexus/content/groups/build.releases/</url>
				</pluginRepository>
			</pluginRepositories>
			<repositories>
				<repository>
					<id>build.releases.repo</id>
					<url>http://nexus:8081/nexus/content/groups/build.releases/</url>

				</repository>
			</repositories>
		</profile>
		<profile>
			<id>milestone.build</id>
			<pluginRepositories>
				<pluginRepository>
					<id>build.milestones.repo</id>

					<url>http://nexus:8081/nexus/content/groups/build.milestones/</url>
				</pluginRepository>
			</pluginRepositories>
			<repositories>
				<repository>
					<id>build.milestones.repo</id>
					<url>http://nexus:8081/nexus/content/groups/build.milestones/</url>

				</repository>
			</repositories>
		</profile>
		<profile>
			<id>snapshot.build</id>
			<pluginRepositories>
				<pluginRepository>
					<id>build.snapshots.repo</id>

					<url>http://nexus:8081/nexus/content/groups/build.snapshots/</url>
				</pluginRepository>
			</pluginRepositories>
			<repositories>
				<repository>
					<id>build.snapshots.repo</id>
					<url>http://nexus:8081/nexus/content/groups/build.snapshots/</url>

				</repository>
			</repositories>
		</profile>
		<profile>
			<id>sonar</id>
				<activation>
					<activeByDefault>true</activeByDefault>
				</activation>

				<properties>
					<sonar.jdbc.url>jdbc:mysql://ldisonarci.wdf.sap.corp:3306/sonar?useUnicode=true&amp;characterEncoding=utf8</sonar.jdbc.url>
					<sonar.jdbc.driver>com.mysql.jdbc.Driver</sonar.jdbc.driver>
					<sonar.jdbc.username>sonar</sonar.jdbc.username>
					<sonar.jdbc.password>sonar</sonar.jdbc.password>
					<sonar.host.url>http://ldisonarci.wdf.sap.corp:8080/sonar</sonar.host.url>

				</properties>
		</profile>
	</profiles>
	<activeProfiles>
		<!-- <activeProfile>release.build</activeProfile> -->
		<!-- <activeProfile>milestone.build</activeProfile> -->
		<activeProfile>snapshot.build</activeProfile>
	</activeProfiles>

	<pluginGroups>
		<pluginGroup>com.sap.ldi</pluginGroup>
	</pluginGroups>
</settings>
